{
  "name": "@theChantu/vite-wasm-pack-watcher",
  "version": "1.0.1",
  "author": "theChantu",
  "homepage": "https://github.com/theChantu/vite-wasm-pack-watcher",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/theChantu/vite-wasm-pack-watcher.git"
  },
  "bugs": {
    "url": "https://github.com/theChantu/vite-wasm-pack-watcher/issues"
  },
  "description": "Watch for Rust file changes and rebuild the project with wasm-pack on changes.",
  "types": "./exports/require.d.cts",
  "main": "./exports/require.cjs",
  "module": "./exports/import.mjs",
  "scripts": {
    "build": "tsc && esbuild dist/index.js --minify --allow-overwrite --outfile=dist/index.js"
  },
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./exports/import.mjs"
    },
    "require": {
      "types": "./exports/require.d.cts",
      "default": "./exports/require.cjs"
    }
  },
  "packageManager": "bun@1.3.8",
  "peerDependencies": {
    "vite": "^2 || ^3 || ^4 || ^5 || ^6"
  },
  "devDependencies": {
    "@types/node": "^22.10.3",
    "esbuild": "^0.24.2",
    "typescript": "^4 || ^5",
    "vite": "^2 || ^3 || ^4 || ^5 || ^6"
  },
  "keywords": [
    "vite",
    "plugin",
    "wasm",
    "webassembly",
    "wasm-pack",
    "es-modules",
    "modules"
  ],
  "files": [
    "/dist",
    "/exports",
    "LICENSE",
    "README.md"
  ]
}
